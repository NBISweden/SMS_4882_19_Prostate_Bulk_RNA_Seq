pages = {563--569},
publisher = {Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
title = {{Nonhybrid, finished microbial genome assemblies from long-read SMRT sequencing data}},
url = {http://dx.doi.org/10.1038/nmeth.2474 10.1038/nmeth.2474 http://www.nature.com/nmeth/journal/v10/n6/abs/nmeth.2474.html{\#}supplementary-information},
