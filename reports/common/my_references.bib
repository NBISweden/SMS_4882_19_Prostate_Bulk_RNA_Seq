issn = {1471-2105},
journal = {BMC bioinformatics},
keywords = {Base Sequence,Internet,Sequence Analysis, DNA,Sequence Analysis, DNA: methods,Sequence Analysis, RNA,Sequence Analysis, RNA: methods,Software},
